<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zyxdimensions.petagram.AcercaDe">

    <ImageView
        android:id="@+id/imgMeme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/meme"
        android:layout_centerHorizontal="true"
        />

    <TextView
        android:id="@+id/multiAutoCompleteTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/imgMeme"
        android:padding="@dimen/padding_acerca_de"
        android:layout_marginTop="@dimen/padding_acerca_de"
        android:text="@string/acerca_de"
        android:textSize="@dimen/letra_acerca_de"
        />
</RelativeLayout>
